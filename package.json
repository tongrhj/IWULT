  },
  "author": "Jared Tong <hello@jaredtong.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-polyfill": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "nodemon": "^1.8.1",
    "standard": "^5.4.1"
  },
  "dependencies": {
    "body-parser": "^1.14.1",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "express-jwt": "^3.3.0",
    "kerberos": "0.0.18",
    "mongoose": "^4.2.8",
    "node-fetch": "^1.3.3",
    "whatwg-fetch": "^0.11.0"
  },
  "standard": {
    "ignore": [
      "/test/*"
    ]
